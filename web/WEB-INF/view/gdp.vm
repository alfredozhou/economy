#* @vtlvariable name="econData" type="economics.model.Observations" *#
<html>
<head>
    <title>GDP Page</title>
<style>
    .BodyTableHeader {
        font-weight: bold;
        font-family: monospace;
        font-size: 18pt;
    }
    .BodyTable {
        border: beige;
    }
</style>
</head>
<body>
<table width="100%" border="0" cellpadding="3" cellspacing="0" class="BodyTable">
    <tr>
        <td class="BodyTableHeader">Year</td>
        <td class="BodyTableHeader">GDP in Billions</td>
    </tr>
    #foreach ($observation in $econData.Observation)
        <td>$observation.date</td>
        <td>$observation.value</td>
    #end
</table>
</body>
</html>